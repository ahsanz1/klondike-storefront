{
  "name": "fabric-klondike",
  "version": "1.0.0",
  "description": "EAT IQBAR Storefront",
  "repository": "https://bitbucket.org/ydv-all/klondike-storefront",
  "author": "MUHAMMAD USMAN BUTT <usman.butt@shopdev.co>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "run-s dev",
    "dev": "env-cmd -f ./.env.dev cross-env NODE_ENV=development node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "start:inspx": "run-s dev:inspx",
    "dev:inspx": "env-cmd -f ./.env.dev cross-env NODE_ENV=development INSPX_ENV=yes node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "uat": "env-cmd -f ./.env.uat cross-env NODE_ENV=development node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "qa": "env-cmd -f ./.env.qa cross-env NODE_ENV=development node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "belley": "env-cmd -f ./.env.belley cross-env NODE_ENV=development node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "prod": "env-cmd -f ./.env.prod cross-env NODE_ENV=development node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "debug": "cross-env NODE_ENV=development webpack-dev-server --config webpack.debug.js",
    "build": "run-s lint build:no-lint",
    "analyze": "run-s lint clean build:analyze",
    "build:no-lint": "run-s clean build:webpack",
    "build:webpack": "env-cmd -f ./.env.dev cross-env NODE_ENV=production BABEL_ENV=post-production node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack -p --config webpack.prod.js",
    "build:webpack:dev": "env-cmd -f ./.env.dev cross-env NODE_ENV=production BABEL_ENV=post-production node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config webpack.prod.js",
    "build:prod": "env-cmd -f ./.env.dev cross-env NODE_ENV=production BABEL_ENV=post-production node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config webpack.analyze.js",
    "clean": "rimraf dist",
    "lint": "run-p lint:js lint:style",
    "lint:js": "eslint \"**/*.@(js|jsx)\"",
    "lint:style": "stylelint \"**/*.@(css|sass|scss)\"",
    "format": "run-p format:js format:style format:others",
    "format:js": "prettier-eslint --write \"**/*.js\"",
    "format:style": "run-s format:style:prettier format:style:stylelint",
    "format:others": "prettier --write \"**/*.@(json|gql|graphql)\"",
    "format:style:prettier": "prettier --write \"**/*.@(css|sass|scss)\"",
    "format:style:stylelint": "stylelint \"**/*.@(css|sass|scss)\" --fix",
    "precommit": "lint-staged",
    "prepush": "run-s lint",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "proxy": "http://localhost:8000",
  "lint-staged": {
    "**/*.@(js|jsx)": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "**/*.@(css|sass|scss)": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "**/*.@(json|gql|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
